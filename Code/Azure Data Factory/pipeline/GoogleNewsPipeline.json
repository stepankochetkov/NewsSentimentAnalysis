{
	"name": "GoogleNewsPipeline",
	"properties": {
		"description": "Master pipeline that orchestrates data integration",
		"activities": [
			{
				"name": "PL_Source_to_BR_GoogleNews",
				"description": "Pipeline to get the source Google news data and put it into Bronze",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "SourceToBronze",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"from": {
							"value": "@pipeline().parameters.date",
							"type": "Expression"
						},
						"to": {
							"value": "@pipeline().parameters.date",
							"type": "Expression"
						},
						"qInTitle": {
							"value": "@pipeline().parameters.topic",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "PL_BR_to_SR_GoogleNews",
				"description": "Pipeline that parses GoogleNews data from BR to SR",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "PL_Source_to_BR_GoogleNews",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "BronzeToSilverRaw",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			},
			{
				"name": "PL_SR_to_ST_Sentimental_Analysis",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "PL_BR_to_SR_GoogleNews",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "SilverRawToSilverTransform",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"source": {
							"value": "@pipeline().parameters.source",
							"type": "Expression"
						},
						"topic": {
							"value": "@pipeline().parameters.topic",
							"type": "Expression"
						},
						"date": {
							"value": "@pipeline().parameters.date",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"source": {
				"type": "string",
				"defaultValue": "Google News"
			},
			"date": {
				"type": "string",
				"defaultValue": "2022-01-18"
			},
			"topic": {
				"type": "string",
				"defaultValue": "Russia OR Russian"
			}
		},
		"annotations": []
	}
}